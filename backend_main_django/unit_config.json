{
    "listeners": {
        "*:80": {
            "pass": "routes/main"
        }
    },
    "routes": {
        "main": [
            {
                "match": {
                    "uri": ["/static/*"]
                },
                "action": {
                    "share": "/webapp/src$uri"
                }
            },
            {
                "match": {
                    "uri": ["/media/*"]
                },
                "action": {
                    "share": "/webapp/src$uri"
                }
            },
            {
                "action": {
                    "pass": "applications/django"
                }
            }
        ]
    },
    "applications": {
        "django": {
            "type": "python",
            "path": "/webapp/src",
            "module": "webapp.wsgi",
            "environment": {
                "DJANGO_SETTINGS_MODULE": "webapp.settings"
            }
        }
    }
}